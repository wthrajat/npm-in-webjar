{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "allowJs": true,
    "moduleResolution": "node",
    "removeComments": true,
    "strictFunctionTypes": false,
    "strictBindCallApply": false,
    "skipLibCheck": true
  }
}